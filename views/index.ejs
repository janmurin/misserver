        <h3 class="text-muted">Zoznam Kontaktov - telefónny zoznam</h3>
    </div>

    <div>
        <div class="row clearfix" style="margin-left: 0px">
            <div class="form-inline">
                <div class="form-group" id="searchFormGroup">
                    <label for="hladanePriezviskoInputText">Zadajte priezvisko hľadanej osoby:</label>


                    <input type="text" class="form-control suggestionsinput" id="hladanePriezviskoInputText"
                           placeholder="Priezvisko"
                           list="priezviska">
                    <span class="glyphicon glyphicon-remove form-control-feedback" aria-hidden="true"></span>
                    <span id="inputError2Status" class="sr-only">(error)</span>
                    <datalist id="priezviska">
                    </datalist>
                </div>
                <button class="btn btn-default" id="hladatButton">Hľadať</button>
            </div>
        </div>
    </div>
    <div class="row" style="display:none" id="tableRow">
        <div id="tableContainer">
            <table class="table table-striped table-bordered table-hover" id="zoznamTable">
                <caption>Nájdené kontakty.</caption>
                <thead>
                <tr>
                    <th>#</th>
                    <th>Meno</th>
                    <th>Pracovisko</th>
                    <th>Email</th>
                    <th>Telefon</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <th scope="row">1</th>
                    <td>Mark</td>
                    <td>Otto</td>
                    <td>@mdo</td>
                    <td>cislo</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>

    <div class="row" style="margin-top: 50px">
        <div class="col-md-6">
            <h4>Top hľadané kontakty</h4>
            <dl id="topHladane">
                <dt>žiadna posledná história</dt>
                <dd></dd>
            </dl>
        </div>
        <div class="col-md-6">
            <h4>Posledná história</h4>
            <dl id="poslednaHistoria">
                <dt>žiadna posledná história</dt>
                <dd></dd>
            </dl>
        </div>
    </div>
    <div class="row" style="margin-top: 50px">
        <h4>Detail kontaktu:</h4>
        <div id="kontaktDetail">

        </div>
    </div>
    <p id="cookie" style="display:none"><%=user.username%></p>
</div>


