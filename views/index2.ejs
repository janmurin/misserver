    }

    .form-control {
        margin-right: auto;
        margin-left: auto;

    }

    .info {
        margin-bottom: 5%;
        font-size: 12pt;
        display: inline-block;
        vertical-align: middle;
        width: 50%;

    }

    .label-success {
        font-size: 12pt;
        line-height: 150%;
    }

    .ramceky {
        margin-left: 5%;
        margin-top: 5%;
        margin-bottom: 10%;
        display: inline-block;
